<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <RootNamespace>DistributionCR</RootNamespace>
    <TargetFramework>net8.0</TargetFramework>
    <ImportedNamespaces>System.Drawing=False,Microsoft.VisualBasic=True,System=True,System.Collections=True,System.Collections.Generic=True,System.Diagnostics=True,System.Threading.Tasks=True,DistributionCR=True</ImportedNamespaces>
    <UseWindowsForms>False</UseWindowsForms>
    <ApplicationIcon>red-cross.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="red-cross.ico" />
  </ItemGroup>

  <ItemGroup>
    <Import Include="System.Drawing" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="EPPlus" Version="7.2.2" />
    <PackageReference Include="SkiaSharp" Version="2.88.8" />
    <PackageReference Include="SkiaSharp.NativeAssets.Linux" Version="2.88.8" />
  </ItemGroup>

</Project>
